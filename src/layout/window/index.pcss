.layout-window {
  position: relative;

  display: flex;

  flex-direction: column;

  align-items: center;
  justify-content: center;

  width: 1180px;
  height: 900px;
  overflow: hidden;
  
  background-color: $darkSecondary;
  border-radius: 10px;
  box-shadow: 0px 4px 4px rgba(0, 0, 0, 0.25);
}
